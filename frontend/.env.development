# Development Environment Variables
VUE_APP_API_BASE_URL=/api
VUE_APP_APP_TITLE=Member Tracker Admin - Development
VUE_APP_APP_VERSION=1.0.0

# Development Features
VUE_APP_ENABLE_DEBUG_TOOLS=true
