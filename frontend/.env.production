# Production Environment Variables
VUE_APP_API_BASE_URL=/api
VUE_APP_APP_TITLE=Member Tracker Admin
VUE_APP_APP_VERSION=1.0.0

# Production Features
VUE_APP_ENABLE_DEBUG_TOOLS=false
VUE_APP_LOG_LEVEL=error

# API Configuration
VUE_APP_API_TIMEOUT=30000
VUE_APP_API_RETRY_ATTEMPTS=3

# Security Configuration
VUE_APP_CSP_ENABLED=true
VUE_APP_SESSION_TIMEOUT=3600000

# Environment
NODE_ENV=production
