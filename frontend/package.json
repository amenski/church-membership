{
  "name": "member-tracker-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Admin backoffice for managing church members, payments, and communications",
  "author": "Your Name",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vue-cli-service serve --mode development",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "build:dev": "vue-cli-service build --mode development",
    "preview": "vite preview --port 4173",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "format": "prettier --write \"src/**/*.{js,vue,json,css,scss,md,ts}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,json,css,scss,md,ts}\"",
    "type-check": "vue-tsc --noEmit",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "clean": "rimraf dist node_modules/.cache",
    "clean:install": "npm run clean && npm install"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "core-js": "^3.33.0",
    "date-fns": "^4.1.0",
    "html2pdf.js": "^0.12.1",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
